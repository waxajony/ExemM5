    <script>
        function getOmonims(txt) {
            document.getElementById('simple_table').hidden = true;
            document.getElementById('result_table').hidden = false;
            if (txt.length < 3) {
                document.getElementById('results').innerHTML = "<tr><th colspan='6'>Iltimos, kamida 3 ta belgi kiriting</th></tr>";
            }
            else {
                URL = "/omonims/api/search/" + txt + "/";
                const xhttp = new XMLHttpRequest();
                xhttp.onload = function() {
                    if (this.responseText == '[]')
                        document.getElementById('results').innerHTML = "<tr><th colspan='6' class='bg-danger'>Ushbu kalitso'z bo'yicha omonim topilmadi</th></tr>";
                    else {
                        var words = JSON.parse(this.responseText);
                        var html_table = ""
                        for (var i = 0; i < words.length; i++) {
                            var html_row = "<tr>" +
                                "<th scope='row' style='vertical-align: middle;'>" + String(i + 1) + "</th>" +
                                "<td class='leksema' style='vertical-align: middle;'>" + words[i]['leksema'] + "</td>" +
                                "<td class='text-center' style='vertical-align: middle;'>" + words[i]['til'] + "</td>" +
                                "<td class='text-center' style='vertical-align: middle;'>" + words[i]['turkum'] + "</td>" +
                                "<td class='mazmun' style='vertical-align: middle;'>" + words[i]['mano'] + "</td>" +
                                "<td class=text-center style='vertical-align: middle;'>" +
                                "<a href='/omonims/" + words[i]['id'] + "/' class='btn btn-success'>" +
                                "<svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' class='bi bi-eye' viewBox='0 0 16 16'>" +
                                "<path d='M16 8s-3-5.5-8-5.5S0 8 0 8s3 5.5 8 5.5S16 8 16 8zM1.173 8a13.133 13.133 0 0 1 1.66-2.043C4.12 4.668 5.88 3.5 8 3.5c2.12 0 3.879 1.168 5.168 2.457A13.133 13.133 0 0 1 14.828 8c-.058.087-.122.183-.195.288-.335.48-.83 1.12-1.465 1.755C11.879 11.332 10.119 12.5 8 12.5c-2.12 0-3.879-1.168-5.168-2.457A13.134 13.134 0 0 1 1.172 8z'/>" +
                                "<path d='M8 5.5a2.5 2.5 0 1 0 0 5 2.5 2.5 0 0 0 0-5zM4.5 8a3.5 3.5 0 1 1 7 0 3.5 3.5 0 0 1-7 0z'/>" +
                                "</svg>" +
                                "</a>" +
                                "</td>" +
                                "</tr>";
                            html_table += html_row;
                        }
                        document.getElementById('results').innerHTML = html_table;
                    }
                }
                    xhttp.open("GET", URL);
                    xhttp.send();
            }

        }
    </script>

